<div class="jumbotron">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Email</th>
        <th scope="col">Moderator</th>
        <th scope="col">Mod/Unmod</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |u| %>
       <% if u.role != 'admin' %>
        <tr class="table-info">
          <th scope="row"><%= u.email %></th>
          <td><%= u.role %></td>
          <td><%= link_to "Update Status" , admin_path(u), method: :patch, data: {confirm: "Are you sure?"} %></td>
        </tr>
      <% end %>
      <% end %>
    </tbody>
  </table> 
</div>